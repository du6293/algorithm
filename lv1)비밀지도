def solution(n, arr1, arr2):
    answer = []
    for i,j in zip(arr1,arr2):
        a12 = str(bin(i|j)[2:])
        a12=a12.rjust(n,'0')
        a12=a12.replace('1','#')
        a12=a12.replace('0',' ')
        answer.append(a12)
    return answer
    
    #bin(A) >> A를 '0b' + 이진수형태 로 변환
    #  val = "123".rjust(5, "a")   >> aa123

    #  val = "222".ljust(15, "a")   >>  222aaaaaaaaaaaa
    #  val = "123".zfill(5)     >> 00123
    
